/* layui 自定义定制 start */
#showIndexTab .layui-tab-close { display: none; } 
.layui-layout-admin .layui-header { background: #393D49; border-bottom: 5px solid #61acff; }
.layui-layout-admin .layui-side { top: 65px; }
.layui-layout-admin .layui-body { top: 65px; }
.layui-body { left: 210px; }
.custom-tab-box>.layui-tab-title .layui-this { color: white; background-color: #61acff; }
.custom-tab-box>.layui-tab-title { border-bottom: 1px solid #61acff; }
.layui-tab-title { height: 39px; }
.layui-tab { margin-bottom: 0; }
#showLayuiTabContent { overflow: auto; background-color: #f1f2f7; }
.layui-tab-title li .layui-tab-close { border-radius: 9px; }
.layui-tab-title li .layui-tab-close:hover { border-radius: 9px; }
.layui-nav.layui-nav-tree .layui-nav-item a { cursor: pointer; }
.show-footer-font { text-align: center; color: #555; }
.layui-logo .show-logo-icon { display: inline-block; margin-top: 17px; width: 111px; height: 27px; vertical-align: sub; background-image: url(../images/logo_icon.png); background-repeat: no-repeat; background-size: 100%; }
.custom-nav-center { margin-left: calc(50% - 304px); }
.custom-nav-center.layui-nav .layui-nav-item { font-size: 24px; }
.layui-header-new .layui-nav-bar { background-color: #393D49; }
.layui-nav.layui-layout-right .layui-nav-item { line-height: 65px; }
.setting-frame-content { background-color: #fff; padding: 20px; border-radius: 5px; border: 1px solid #eee; }
.layui-form.font-num-6 .layui-form-label { width: 84px; }
.layui-form.font-num-6 .layui-input-block { margin-left: 114px; }
.layui-form-radio>i:hover, .layui-form-radioed>i { color: #61acff; }
.layui-form-select dl dd.layui-this { background-color: #61acff; }
.layui-nav.layui-nav-tree .layui-nav-item dd .show-new-tab { margin-left: 10px; }
/* layui 自定义定制 end */
/* layer弹窗自定义 start */
.layuiWindownContent { padding: 20px; }
.layui-layer-btn .layui-layer-btn0 { border-color: #61acff!important; background-color: #61acff!important; }
/* layer 弹窗自定义 end */

